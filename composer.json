{
    "name": "ljvicente/tic-tac-toe",
    "description": "Simple Tic Tac Toe game written in PHP",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "illuminate/session": "^5.7",
        "laravel/lumen-framework": "5.7.*",
        "vlucas/phpdotenv": "~2.2"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "~1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "TicTacToe\\": "src/ljvicente/TicTacToe"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
